{
  "name": "chatgpt-api-prompts",
  "version": "0.0.1",
  "description": "A NodeJS ChatGPT prompts library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "watch": "npm run build -- --watch src",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pacholoamit/chatgpt-api-prompts.git"
  },
  "author": "pacholo amit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pacholoamit/chatgpt-api-prompts/issues"
  },
  "homepage": "https://github.com/pacholoamit/chatgpt-api-prompts#readme",
  "devDependencies": {
    "tsup": "^6.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "chatgpt": "^4.1.1"
  }
}
