{
  "name": "chatgpt-prompts",
  "version": "1.0.0",
  "description": "A NodeJS ChatGPT prompts library",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "scrape": "rimraf prompts.txt && rimraf imports.txt && ts-node scripts/scrape.ts",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "watch": "npm run build -- --watch src",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pacholoamit/chatgpt-prompts.git"
  },
  "author": "pacholo amit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pacholoamit/chatgpt-prompts/issues"
  },
  "homepage": "https://github.com/pacholoamit/chatgpt-prompts#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.19",
    "axios": "^1.3.2",
    "csv": "^6.2.6",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.3",
    "fs": "^0.0.1-security",
    "lodash": "^4.17.21",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "chatgpt": "^4.1.1"
  },
  "keywords": [
    "openai",
    "chatgpt",
    "chat",
    "gpt",
    "gpt-3",
    "gpt3",
    "gpt4",
    "chatbot",
    "machine learning",
    "conversation",
    "conversational ai",
    "ai",
    "ml",
    "bot"
  ]
}
